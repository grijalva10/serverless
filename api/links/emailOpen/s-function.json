{
  "name": "emailOpen",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: MoonMail-v2",
  "customName": "${project}-links-emailOpen",
  "customRole": false,
  "handler": "links/emailOpen/handler.default",
  "timeout": 20,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "cors": "$${corsSettings}"
  },
  "endpoints": [
    {
      "path": "links/open/{campaignId}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "campaignId": "$input.params('campaignId')",
          "recipientId": "$input.params('r')",
          "userId": "$input.params('u')",
          "listId": "$input.params('l')",
          "segmentId": "$input.params('s')",
          "headers": "{#foreach($header in $input.params().header.keySet())\"$header\": \"$util.escapeJavaScript($input.params().header.get($header))\" #if($foreach.hasNext),#end#end}"
        }
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "204"
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "OPENS_STREAM_NAME": "${opensStreamName}",
    "LOG_LEVEL": "error"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
