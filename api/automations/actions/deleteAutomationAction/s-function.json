{
  "name": "deleteAutomationAction",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: MoonMail-v2",
  "customName": "${project}-automations-deleteAutomationAction",
  "customRole": false,
  "handler": "automations/actions/deleteAutomationAction/handler.default",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "cors": "$${corsSettings}"
  },
  "endpoints": [
    {
      "path": "/automations/{automationId}/actions/{actionId}",
      "method": "DELETE",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json":  {
          "automationId": "$input.params('automationId')",
          "actionId": "$input.params('actionId')",
          "action": "$input.json('$')",
          "authToken": "$input.params('Authorization')"
        }
      },
      "responses": "$${responses}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "AUTOMATIONS_TABLE": "$${automationsTable}",
    "AUTOMATION_ACTIONS_TABLE": "$${automationActionsTable}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
