{
  "name": "getRecipient",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: MoonMail-v2",
  "customName": "${project}-recipients-getRecipient",
  "customRole": false,
  "handler": "recipients/getRecipient/handler.default",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "cors": "$${corsSettings}"
  },
  "endpoints": [
    {
      "path": "/lists/{listId}/recipients/{recipientId}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "listId": "$input.params('listId')",
          "recipientId": "$input.params('recipientId')",
          "options": {
            "fields": "$input.params('fields')",
            "include_fields": "$input.params('include_fields')"
          },
          "authToken": "$input.params('Authorization')"
        }
      },
      "responses": "$${responses}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "DEBUG": "true",
    "RECIPIENTS_TABLE": "$${recipientsTable}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
