{
  "name": "attachRecipients",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: MoonMail-v2",
  "customName": false,
  "customRole": false,
  "handler": "recipients/attachRecipients/handler.default",
  "timeout": 300,
  "memorySize": 1536,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [],
  "events": [
    {
      "name": "attach-recipients-topic",
      "type": "sns",
      "config": {
        "topicName": "$${attachRecipientsTopic}"
      }
    }
  ],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "ATTACH_LIST_RECIPIENTS_TOPIC_ARN": "${attachListRecipientsTopicARN}",
    "ATTACH_SEGMENTS_RECIPIENTS_TOPIC_ARN": "${attachSegmentRecipientsTopicARN}",
    "SEND_EMAILS_TOPIC_ARN": "${sendEmailsTopicARN}",
    "UPDATE_CAMPAIGN_TOPIC_ARN": "${updateCampaignTopicARN}",
    "RECIPIENTS_TABLE": "$${recipientsTable}",
    "DEBUG": "true"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
